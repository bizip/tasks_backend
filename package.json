{
  "name": "node_templete",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/app.js",
    "start": "babel-node  src/app.js",
    "test": "mocha --require @babel/register 'src/tests/**/*.test.js'",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:unseed": "sequelize db:seed:undo:all",
    "db:migrate:undo": "sequelize db:migrate:undo:all",
    "db:reset": "npm run db:migrate:undo && npm run db:migrate && npm run db:seed",
    "db:hard:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bizip/node_templete.git"
  },
  "author": "Bizimungu Pascal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bizip/node_templete/issues"
  },
  "homepage": "https://github.com/bizip/node_templete#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "mocha": "^10.2.0",
    "nodemailer": "^6.9.3",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.0",
    "start": "babel-node  src/app.js",
    "swagger-ui-express": "^4.6.2",
    "test": "mocha --require @babel/register 'src/tests/**/*.test.js'"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "@babel/register": "^7.21.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.4",
    "sequelize-cli": "^6.6.0"
  }
}
